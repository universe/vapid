{
  "name": "@neutrinodev/dashboard",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "description": "A template-driven content management system",
  "repository": "universe/vapid",
  "author": "Adam Miller <adam@universe.app>",
  "source": "src/index.html",
  "homepage": "https://website.universe.app",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/javascripts/dashboard.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint bin lib test",
    "test": "jest | pino-pretty",
    "emulate": "firebase emulators:start",
    "start": "yarn clean && vite --force",
    "build": "yarn clean && vite build && rm -f tsconfig.tsbuildinfo && tsc --emitDeclarationOnly -p tsconfig.json",
    "deploy": "yarn build && hoist up dist"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "app",
    "cms",
    "framework",
    "web"
  ],
  "nodemonConfig": {
    "ignore": [
      "assets/*"
    ]
  },
  "files": [
    "assets",
    "bin",
    "dist"
  ],
  "peerDependencies": {
    "firebase": "^10",
    "preact": "^10"
  },
  "devDependencies": {
    "@neutrinodev/cli": "1.0.0-beta.0",
    "@neutrinodev/core": "1.0.0-beta.0",
    "@neutrinodev/runtime": "1.0.0-beta.0",
    "@universe/aether": "^0.15.2",
    "@universe/util": "^1.1.2",
    "colorthief": "^2.3.2",
    "file2md5": "^1.3.0",
    "html5sortable": "^0.13.2",
    "preact": "^10.19.3",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^4.1.2",
    "react-hot-toast": "^2.2.0",
    "react-spring": "^9.5.2",
    "react-text-transition": "^3.0.2",
    "react-with-gesture": "^4.0.8",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@preact/preset-vite": "^2.3.0",
    "@stripe/react-stripe-js": "^1.16.2",
    "@stripe/stripe-js": "^1.46.0",
    "@types/colorthief": "link:../../../types-local/colorthief",
    "@types/html-escaper": "^3.0.0",
    "@types/html5sortable": "link:../../../types-local/html5sortable",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "simple-dom": "^1.4.0"
  }
}
